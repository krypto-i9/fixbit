{
  "name": "bug-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "connected-react-router": "^6.8.0",
    "notistack": "^1.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "copy-api": "cp -r api/users/* /var/www/html/api/users/",
    "build-copy": "react-scripts build && cp -r build/* /var/www/html/",
    "pre-build": "react-scripts build && cd pre_build && mkdir hyperv && cd ../ && cp -r build/* pre_build/hyperv/ && cp -r api pre_build/hyperv/ && cd pre_build && zip -r hyperv.zip hyperv/ && rm -r -f hyperv && cd ../",
    "production-build": "react-scripts build && mkdir production_build && cp -r build/* production_build/ && cp -r api production_build/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
